{
	"name": "@ms-covidbot/service",
	"private": true,
	"containerName": "ms-covid-healthbot-service",
	"version": "0.0.8",
	"description": "",
	"scripts": {
		"build:webpack": "webpack",
		"build:docker": "docker build -t ${AZURE_CONTAINER_REGISTRY}/$(node -p 'require(\"./package.json\").containerName') .",
		"bundle:service": "run-s build:webpack build:docker",
		"clean": "rimraf dist",
		"build": "webpack",
		"start:webpack": "webpack -w",
		"start:func": "func start",
		"start:docker": "docker run -t -i --rm -p 7071:80 --env-file .env ${AZURE_CONTAINER_REGISTRY}/$(node -p 'require(\"./package.json\").containerName')",
		"start": "run-p start:webpack start:func",
		"deploy": "yarn node ./scripts/deploy.js -c ${AZURE_CONTAINER_REGISTRY} -i $(node -p 'require(\"./package.json\").containerName') -u ${DOCKER_USER} -p ${DOCKER_PASSWORD}"
	},
	"devDependencies": {
		"@azure/functions": "^1.2.3",
		"@ms-covidbot/policy-locator": "workspace:packages/locator",
		"@ms-covidbot/state-plan-schema": "workspace:packages/schema",
		"@types/debug": "^4.1.5",
		"@types/lru-cache": "^5.1.0",
		"@types/minimist": "^1.2.1",
		"@types/node": "^14.14.22",
		"@types/node-fetch": "^2.5.8",
		"@types/rimraf": "^3.0.0",
		"azure-functions-core-tools": "^3.0.3233",
		"debug": "^4.3.2",
		"execa": "^5.0.0",
		"minimist": "^1.2.5",
		"minimist-options": "^4.1.0",
		"npm-run-all": "^4.1.5",
		"rimraf": "^3.0.2",
		"ts-loader": "^8.0.14",
		"typescript": "^4.1.3",
		"webpack": "^5.20.1",
		"webpack-cli": "^4.5.0"
	},
	"dependencies": {
		"@azure/storage-blob": "^12.4.0",
		"azure-function-log-intercept": "^1.0.7",
		"dotenv": "^8.2.0",
		"lru-cache": "^6.0.0",
		"node-fetch": "^2.6.1"
	}
}
