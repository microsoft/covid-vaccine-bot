  <script>
    window.onload = function () {
      // Declaration
      var FINDER = document.querySelector(".js-covid-test-finder");
      if (!FINDER === null) {
        var LABEL = FINDER.querySelector(".js-covid-test-finder-label"),
        INPUT = FINDER.querySelector(".js-covid-test-finder-input"),
        BUTTON = FINDER.querySelector(".js-covid-test-finder-button"),
        ERROR = FINDER.querySelector(".js-form-error");
      // Attach Events
      BUTTON.addEventListener("click", function () {
        checkZip();
      });
      INPUT.addEventListener("keydown", function (e) {
        if (e.key === "Enter") {
          checkZip();
        }
      });
      INPUT.addEventListener("keyup", function (e) {
        const val = e.target.value;
        if (val.length < 6) {
          INPUT.value = e.target.value;
        }
  
        if (val.length > 5) {
          INPUT.value = INPUT.value.replace("-", "");
          INPUT.value =
            INPUT.value.substring(0, 5) +
            "-" +
            INPUT.value.substring(5, INPUT.value.length);
        }
      });
      // Check valid zipcode
      function checkZip() {
        var zip = INPUT.value;
        var isValidZip = /(^\d{5}$)|(^\d{5}-\d{4}$)/.test(zip);
        console.log(isValidZip);
        if (isValidZip) {
          FINDER.classList.remove("alert");
          LABEL.classList.remove("is-invalid-label");
          INPUT.classList.remove("is-invalid-input");
          ERROR.classList.remove("is-visible");
          find(zip);
        } else {
          FINDER.classList.add("alert");
          LABEL.classList.add("is-invalid-label");
          INPUT.classList.add("is-invalid-input");
          ERROR.classList.add("is-visible");
        }
      }
      // Forward to 211 url
      function find(zip) {
        var URL =
          "https://www.211ct.org/search?terms=COVID-19%20Diagnostic%20Tests&page=1&location=" +
          zip +
          "&taxonomy_code=11048&service_area=hartford";
        window.open(URL, "_blank");
      }
    };
};
  </script>
